 #!/bin/bash
while(sleep 10);
	do
		sleep 2;
		wget http://www.yr.no/sted/Norge/Oslo/Oslo/Oslo/varsel.xml -o ./varsel.xml;
		sleep 2;
		cat ./varsel.xml |grep ".tabular." -A 11|head -n9|grep .symbol|awk '{print $3}'|cut -d\" -f2|while read l; do echo $l > ./status;done;
		cat ./varsel.xml |grep ".tabular." -A 11|head -n9|grep windSpeed|awk '{print $3" "$4}'|cut -d\" -f 2 |while read i; do echo $i > ./vind;done;
		cat ./varsel.xml |grep ".tabular." -A 11|head -n9|grep .temperature|awk '{print $3}'|cut -d\" -f2 |while read j; do echo $j > ./temperatur;done;
		sleep 2;
		rm -f ./varsel.xml*;
	done;
